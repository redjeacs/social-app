generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id              String      @id @default(uuid())
  email           String      @unique
  username        String      @unique
  firstName       String
  lastName        String
  password        String
  createdAt       DateTime    @default(now())

  @@map("app_users")
}